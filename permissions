permissions:
  hunteranticheat.system.notice:
    description: See broadcasts when a player is kicked or banned for hacking
  hunteranticheat.system.alert:
    description: See alerts when a player might be cheating
  hunteranticheat.system.help:
    description: Access the help message
  hunteranticheat.system.mute:
    description: Access the mute function
  hunteranticheat.system.report:
    description: Access cheat reports
  hunteranticheat.mod:
    description: Mod helper perm
    children:
      hunteranticheat.system.notice: true
      hunteranticheat.system.alert: true
      hunteranticheat.system.help: true
      hunteranticheat.system.report: true
      hunteranticheat.system.mute: true
  hunteranticheat.system.log:
    description: Access turning console logging on or off
  hunteranticheat.system.reload:
    description: Access reloading the system
  hunteranticheat.system.reset:
    description: Access resetting a player's cheat level
  hunteranticheat.system.version:
    description: Access information about whether the system is updated
  hunteranticheat.admin:
    description: Admin helper perm
    children:
      hunteranticheat.mod: true
      hunteranticheat.system.log: true
      hunteranticheat.system.reload: true
      hunteranticheat.system.reset: true
      hunteranticheat.system.mute: true
  hunteranticheat.check.flight:
    description: Allow players to use fly cheats
    default: false
  hunteranticheat.check.elytrafly:
    description: Allow players to use ElytraFly cheats
    default: false
  hunterhunteranticheat.check.waterwalk:
